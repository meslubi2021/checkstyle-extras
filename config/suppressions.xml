<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
  <suppress files="[\\/]parsers[\\/]\w+Parser\.java" checks="ClassDataAbstractionCoupling" />

  <suppress files="[\\/]visitors[\\/]\w+AstVisitor\.java" checks="LambdaBodyLength" />

  <suppress files="[\\/]walkers[\\/]\w+Walker\.java" checks="GenericWhitespace"
    message="'>'" />

  <!-- Indentation controlled by Eclipse. -->
  <suppress checks="IndentationCheck" />
  <suppress checks="TrailingComment" />
  <!-- Not implementing languages yet. -->
  <suppress checks="Translation" />
</suppressions>
